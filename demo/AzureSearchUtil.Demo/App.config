<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <appSettings>
    <!--AzureSearch Settings-->

    <!--Primary admin key as Search API key (eg. CE52AF5F39A41ED5B479BAA7DA3955D6)-->
    <add key="AzureSearchApiKey" value="" />

    <!--Search URL prefix. Eg. https://test123.search.windows.net-->
    <add key="AzureSearchUrlPrefix" value="" />
    
    <add key="AzureSearchApiVersion" value="2014-07-31-Preview" />
    <add key="AzureSearchBatchUpdateLimit" value="5" />
    
  </appSettings>

</configuration>